---
import _ from 'lodash';
import { getCollection } from 'astro:content';
import Layout from '../layouts/Layout.astro';
import { ZinePage } from '../templates/ZinePage';

const entries = await getCollection('zines');
---

<Layout url="/zine/" title="From dawn till coffee photography zine">
	<ZinePage
		url="/zine/"
		title="From dawn till coffee photography zine"
		issues={entries}
	/>
</Layout>
