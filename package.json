{
  "name": "sapegin",
  "private": true,
  "license": "See license in <Readme.md>",
  "scripts": {
    "start": "gatsby develop",
    "pretest": "npm run lint",
    "test": "npm run typecheck",
    "posttest": "npm run format",
    "typecheck": "tsc",
    "build": "gatsby build",
    "lint": "eslint . --ext .js,.tsx --cache --fix",
    "format": "prettier --loglevel warn --write \"**/*.{js,md}\"",
    "styleguide": "styleguidist server",
    "styleguide:build": "styleguidist build"
  },
  "engines": {
    "node": ">=10"
  },
  "browserslist": [
    ">1%",
    "last 1 version",
    "Firefox ESR",
    "not dead"
  ],
  "dependencies": {
    "@styled-system/css": "^5.1.5",
    "@types/styled-system__css": "^5.0.11",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-group": "^3.0.2",
    "react-helmet": "^6.1.0",
    "sort-on": "^4.1.0",
    "styled-components": "^5.1.1",
    "tamia": "5.9.1",
    "tamia-gatsby-link": "^1.0.6"
  },
  "devDependencies": {
    "@types/react": "^16.9.38",
    "@types/react-dom": "^16.9.8",
    "@types/react-helmet": "^6.0.0",
    "@types/styled-components": "^5.1.0",
    "@typescript-eslint/eslint-plugin": "^3.3.0",
    "@typescript-eslint/parser": "^3.3.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.2.0",
    "eslint-config-tamia": "^7.2.5",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-react": "^7.20.0",
    "gatsby": "2.23.4",
    "gatsby-plugin-fathom": "^1.3.0",
    "gatsby-plugin-netlify": "2.3.5",
    "gatsby-plugin-react-helmet": "^3.3.4",
    "gatsby-plugin-styled-components": "^3.3.4",
    "gatsby-plugin-typescript": "^2.4.6",
    "gatsby-source-filesystem": "2.3.11",
    "gatsby-transformer-remark": "2.8.16",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "prettier": "^2.0.5",
    "react-styleguidist": "^11.0.8",
    "typescript": "^3.9.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,tsx,md}": "prettier --write",
    "*.{js,tsx}": "eslint --cache --fix"
  }
}
