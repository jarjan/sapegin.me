{
  "name": "sapegin",
  "private": true,
  "license": "See license in <Readme.md>",
  "scripts": {
    "start": "gatsby develop",
    "pretest": "npm run lint",
    "test": "npm run typecheck",
    "posttest": "npm run format",
    "typecheck": "tsc",
    "build": "gatsby build",
    "lint": "eslint . --ext .js,.tsx --cache --fix",
    "format": "prettier --write \"**/*.{js,tsx,md}\"",
    "styleguide": "styleguidist server",
    "styleguide:build": "styleguidist build"
  },
  "engines": {
    "node": ">=8.9"
  },
  "browserslist": [
    ">1%",
    "last 1 version",
    "Firefox ESR",
    "not dead"
  ],
  "dependencies": {
    "@styled-system/css": "^5.1.5",
    "@types/styled-system__css": "^5.0.5",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-group": "^3.0.2",
    "react-helmet": "^5.2.1",
    "sort-on": "^4.1.0",
    "styled-components": "^5.0.1",
    "tamia": "5.4.0",
    "tamia-gatsby-link": "^1.0.6"
  },
  "devDependencies": {
    "@types/react": "^16.9.22",
    "@types/react-helmet": "^5.0.15",
    "@types/styled-components": "^5.0.0",
    "@typescript-eslint/eslint-plugin": "^2.21.0",
    "@typescript-eslint/parser": "^2.21.0",
    "babel-eslint": "^10.0.3",
    "eslint": "^6.8.0",
    "eslint-config-tamia": "^7.1.1",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-react": "^7.18.3",
    "gatsby": "2.19.21",
    "gatsby-plugin-fathom": "^1.2.0",
    "gatsby-plugin-netlify": "2.1.32",
    "gatsby-plugin-react-helmet": "^3.1.22",
    "gatsby-plugin-styled-components": "^3.1.19",
    "gatsby-plugin-typescript": "^2.1.27",
    "gatsby-source-filesystem": "2.1.48",
    "gatsby-transformer-remark": "2.6.53",
    "husky": "^4.2.3",
    "lint-staged": "^9.4.2",
    "prettier": "^1.19.1",
    "react-styleguidist": "^10.6.2",
    "typescript": "^3.8.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,tsx,md}": [
      "prettier --write",
      "git add"
    ],
    "*.{js,tsx}": [
      "eslint --fix",
      "git add"
    ]
  }
}
