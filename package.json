{
  "name": "sapegin",
  "private": true,
  "license": "See license in <Readme.md>",
  "scripts": {
    "start": "gatsby develop",
    "pretest": "npm run lint:js && npm run lint:css",
    "test": "npm run typecheck",
    "posttest": "npm run format",
    "typecheck": "tsc",
    "build": "gatsby build",
    "lint:js": "eslint . --ext .js,.tsx --cache --fix",
    "lint:css": "stylelint \"src/**/*.tsx\"",
    "format": "prettier --write \"**/*.{js,tsx,md}\"",
    "styleguide": "styleguidist server",
    "styleguide:build": "styleguidist build"
  },
  "engines": {
    "node": ">=8.9"
  },
  "browserslist": [
    ">1%",
    "last 1 version",
    "Firefox ESR",
    "not dead"
  ],
  "dependencies": {
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-group": "^3.0.2",
    "react-helmet": "^5.2.1",
    "styled-components": "^4.4.1",
    "tamia": "5.0.20",
    "tamia-gatsby-link": "^1.0.6"
  },
  "devDependencies": {
    "@types/react": "^16.9.11",
    "@types/react-helmet": "^5.0.14",
    "@types/styled-components": "^4.1.20",
    "@typescript-eslint/eslint-plugin": "^2.6.1",
    "@typescript-eslint/parser": "^2.6.1",
    "babel-eslint": "^10.0.3",
    "eslint": "^6.6.0",
    "eslint-config-tamia": "^7.1.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-react": "^7.16.0",
    "gatsby": "2.17.8",
    "gatsby-plugin-fathom": "^1.1.0",
    "gatsby-plugin-lodash": "3.1.13",
    "gatsby-plugin-netlify": "2.1.23",
    "gatsby-plugin-react-helmet": "^3.1.13",
    "gatsby-plugin-styled-components": "^3.1.11",
    "gatsby-plugin-typescript": "^2.1.15",
    "gatsby-source-filesystem": "2.1.35",
    "gatsby-transformer-remark": "2.6.32",
    "husky": "^3.0.9",
    "lint-staged": "^9.4.2",
    "prettier": "^1.18.2",
    "react-styleguidist": "^10.2.0",
    "stylelint": "^11.1.1",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.8.0",
    "typescript": "^3.6.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,tsx,md}": [
      "prettier --write",
      "git add"
    ],
    "*.{js,tsx}": [
      "stylelint --fix",
      "eslint --fix",
      "git add"
    ]
  }
}
