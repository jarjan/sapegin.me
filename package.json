{
  "name": "sapegin",
  "private": true,
  "license": "See license in <Readme.md>",
  "scripts": {
    "start": "gatsby develop",
    "pretest": "npm run lint:js && npm run lint:css",
    "test": "npm run typecheck",
    "posttest": "npm run format",
    "typecheck": "tsc",
    "build": "gatsby build",
    "lint:js": "eslint . --ext .js,.tsx --cache --fix",
    "lint:css": "stylelint \"src/**/*.tsx\"",
    "format": "prettier --write \"**/*.{js,tsx,md}\"",
    "styleguide": "styleguidist server",
    "styleguide:build": "styleguidist build"
  },
  "engines": {
    "node": ">=8.9"
  },
  "browserslist": [
    ">1%",
    "last 1 version",
    "Firefox ESR",
    "not dead"
  ],
  "dependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-group": "^3.0.2",
    "react-helmet": "^5.2.1",
    "styled-components": "^4.3.2",
    "tamia": "5.0.0-alpha.13",
    "tamia-gatsby-link": "^1.0.6"
  },
  "devDependencies": {
    "@types/react": "^16.8.23",
    "@types/react-helmet": "^5.0.8",
    "@types/styled-components": "^4.1.18",
    "@typescript-eslint/eslint-plugin": "^1.12.0",
    "@typescript-eslint/parser": "^1.12.0",
    "babel-eslint": "^10.0.2",
    "eslint": "^5.16.0",
    "eslint-config-tamia": "^6.2.1",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-react": "^7.14.2",
    "gatsby": "2.13.27",
    "gatsby-plugin-fathom": "^1.1.0",
    "gatsby-plugin-lodash": "3.1.2",
    "gatsby-plugin-netlify": "2.1.3",
    "gatsby-plugin-react-helmet": "^3.1.2",
    "gatsby-plugin-remove-trailing-slashes": "2.1.2",
    "gatsby-plugin-styled-components": "^3.1.2",
    "gatsby-plugin-typescript": "^2.1.2",
    "gatsby-source-filesystem": "2.1.5",
    "gatsby-transformer-remark": "2.6.6",
    "husky": "^3.0.0",
    "lint-staged": "^9.2.0",
    "prettier": "^1.18.2",
    "react-styleguidist": "^9.1.11",
    "stylelint": "^10.1.0",
    "stylelint-config-recommended": "^2.2.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.8.0",
    "typescript": "^3.5.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,tsx,md}": [
      "prettier --write",
      "git add"
    ],
    "*.{js,tsx}": [
      "stylelint --fix",
      "eslint --fix",
      "git add"
    ]
  }
}
