{
  "name": "sapegin",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "tamia server & npm run build:watch",
    "test": "npm run lint",
    "bundle": "tamia bundle",
    "build": "node lib",
    "build:watch": "chokidar templates source -c 'node lib'",
    "compile": "babel -d lib src",
    "compile:watch": "babel --watch -d lib src",
    "lint": "eslint js lib"
  },
  "dependencies": {
    "tamia": "~2.0.0"
  },
  "devDependencies": {
    "babel-cli": "~6.4.0",
    "babel-eslint": "~5.0.0",
    "babel-preset-tamia": "~6.5.0",
    "chokidar-cli": "~1.2.0",
    "eslint": "~1.10.3",
    "eslint-config-tamia": "~1.1.0",
    "fledermaus": "~4.1.0",
    "tamia-build": "~0.1.0"
  }
}
